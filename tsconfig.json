{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
    "noImplicitThis": false
  },
  "include": ["src/**/*", "src/**/*.spec.ts"],
  "exclude": ["node_modules"]
}
